{"index.html":"<html><head><script src=\"//ej2.syncfusion.com/javascript/demos/list-box/template/datasource.js\" type=\"text/javascript\"></script>\n<script src=\"https://cdn.syncfusion.com/ej2/27.1.48/dist/ej2.min.js\" type=\"text/javascript\"></script>\n    <link href=\"https://cdn.syncfusion.com/ej2/27.1.48/material.css\" rel=\"stylesheet\">\n\n    <link href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\" rel=\"stylesheet\">\n\n    <style>\n            body{\n                touch-action:none;\n            }\n            .control-section{\n                margin-top:100px;\n            }\n        </style></head><body><div class=\"stackblitz-container {{theme}}\"><div class=\"col-lg-12 control-section\">\n    <div id=\"listbox-control\">\n        <input id=\"listbox\">\n    </div>\n</div>\n</div>\n\n\n\n\n<style>\n    #listbox-control {\n        width: 350px;\n        margin: auto;\n    }\n\n    @media screen and (max-width: 590px) {\n        #listbox-control {\n            width: 100%;\n        }\n    }\n    \n    .list-wrapper {\n        height: inherit;\n        position: relative;\n        padding: 14px 12px 14px 78px;\n    }\n    \n    .list-wrapper .text,\n    .list-wrapper .description {\n        display: block;\n        margin: 0;\n        padding-bottom: 3px;\n        white-space: normal;\n    }\n    \n    .list-wrapper .description {\n        font-size: 12px;\n        font-weight: 500;\n    }\n    \n    #listbox-control .e-listbox-wrapper .list-wrapper .text {\n        font-weight: bold;\n        font-size: 13px;\n    }\n    \n    .list-wrapper .e-avatar {\n        position: absolute;\n        left: 5px;\n        background-color: transparent;\n        font-size: 22px;\n        top: calc(50% - 26px);\n    }\n    \n    #listbox-control .javascript {\n        background-image: url('//ej2.syncfusion.com/javascript/demos/src/list-box/images/javascript.svg');\n        width: 50px;\n        height: 50px;\n        background-repeat: no-repeat;\n        margin-left: 10px;\n    }\n    \n    #listbox-control .typescript {\n        background-image: url('//ej2.syncfusion.com/javascript/demos/src/list-box/images/typescript.svg');\n        width: 50px;\n        height: 50px;\n        background-repeat: no-repeat;\n        margin-left: 10px;\n    }\n    \n    #listbox-control .angular {\n        background-image: url('//ej2.syncfusion.com/javascript/demos/src/list-box/images/angular.svg');\n        width: 50px;\n        height: 50px;\n        background-repeat: no-repeat;\n        margin-left: 10px;\n    }\n    \n    #listbox-control .vue {\n        background-image: url('//ej2.syncfusion.com/javascript/demos/src/list-box/images/vue.svg');\n        width: 50px;\n        height: 50px;\n        background-repeat: no-repeat;\n        margin-left: 10px;\n    }\n    \n    #listbox-control .react {\n        background-image: url('//ej2.syncfusion.com/javascript/demos/src/list-box/images/react.svg');\n        width: 50px;\n        height: 50px;\n        background-repeat: no-repeat;\n        margin-left: 10px;\n    }\n</style></body></html>","index.js":"{{ripple}}/**\n * ListBox Template Sample\n */\n\n \n\n    // Initialize ListBox component.\n    var listObj= new ej.dropdowns.ListBox({ \n        // Set the info to the data source property.\n        dataSource: window.template,\n        // set the template content for list items.\n        itemTemplate:  '<div class=\"list-wrapper\">' +\n        '<span class=\"${pic} e-avatar e-avatar-xlarge e-avatar-circle\"></span>' +\n        '<span class=\"text\">${text}</span><span class=\"description\">' +\n        '${description}</span></div>'\n    });\n    listObj.appendTo('#listbox');\n\n\n"}